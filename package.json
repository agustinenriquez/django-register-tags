{
	"name": "django-register-tags",
	"displayName": "django register tags",
	"publisher": "agus",
	"description": "register admin models easily",
	"repository": {
		"type": "git",
		"url": "git://github.com/m0tz/django-register-tags.git"
	},
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:django-register-tags.registerModel",
		"onCommand:django-register-tags.registerModels"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "django-register-tags.registerModel",
				"title": "Register Model"
			},
			{
				"command": "django-register-tags.registerModels",
				"title": "Register Models"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "django-register-tags.registerModel",
					"group": "navigation"	
				},
				{
					"command": "django-register-tags.registerModels",
					"group": "navigation"	
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
